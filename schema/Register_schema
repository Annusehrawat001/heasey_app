from pydantic import BaseModel,EmailStr
from typing import Optional

class RegisterA(BaseModel):
    name:str
    email:EmailStr
    password:str
    phone:str
    pincode:int
    Address:str
    state:str
    gender:str

class Update_Date(BaseModel):
    email:Optional[str]=None
    password:Optional[str]=None
    
